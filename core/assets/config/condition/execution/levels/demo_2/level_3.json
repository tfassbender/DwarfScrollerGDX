    demo_2_level_3_start_boss_cutscene: {
        conditionId: demo_2_level_3_boss_cutscene_executed,
        thenCase: {
            type: NO_EXECUTION,
        },
        elseCase: {
            type: EVENT,
            eventConfig: {
                executionType: START_CUTSCENE,
                executionParameters: {
					cutsceneId: demo_2_level_3_entered_boss_area,
				}
            }
        }
    },
    
    demo_2_level_3_top_right_repeated_entering: {
